{
  "name": "server4test",
  "description": "server for test",
  "version": "0.8.6",
  "files": [
    "lib"
  ],
  "author": "Codenautas <codenautas@googlegroups.com>",
  "repository": "codenautas/server4test",
  "license": "MIT",
  "contributors": [
    {
      "name": "Emilio Platzer",
      "email": "emilioplatzer@gmail.com"
    }
  ],
  "main": "lib/server4test.js",
  "types": "lib/server4test.d.ts",
  "bin": {
    "server4test": "./lib/launch4test.js"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.17.2",
    "fs-extra": "^10.0.0",
    "serve-index": "^1.9.1",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-dev-middleware": "^5.3.0",
    "best-globals": "^1.0.2",
    "errores": "^0.0.2",
    "mini-tools": "^1.11.2",
    "serve-content": "^0.3.17"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.8",
    "@types/serve-index": "^1.9.1",
    "es5-shim": "^4.6.4",
    "fetch-ie8": "^1.5.0",
    "whatwg-fetch": "^3.6.2",
    "require-bro": "^0.3.0",
    "types.d.ts": "^0.6.6",
    "mixin-patch": "^0.4.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "start": "(mkdir local-file-store || echo 'using local-file-store) && node lib/launch4test.js",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "test": "mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "example": "tsc -p example && node example/local-built/example/example-server.js",
    "prepare": "tsc && mixin-patch",
    "x-prepare": "(tsc || echo 'prepare with errors')",
    "mixin-patch": "mixin-patch"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 0,
    "run-in": "client",
    "stability": "extending",
    "type": "lib"
  },
  "mixin-patch": {
    "patch": [
      "lib/launch4test.js"
    ]
  }
}
